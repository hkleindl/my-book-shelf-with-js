
<img src="data:image/png;base64,">
        
      
<div class="card">
  <h3><%= link_to book.name, book_path(book) %></h3>
      <% if user.books.include?(book) %>
        <p>My Rating: <%= pluralize(book.ratings.where(user_id: params[:id]).pluck(:stars)[0], 'star') %></p> 
      <% end %>
      
    <div class="book-button"
    data-book-id="<%= book.id %>"> 
      <button onclick="showDetails(<%= book.id %>,'<%= list_name %>')">Details</button>
    </div><br>
  <div style="display: none" id="<%= list_name %>-<%= book.id %>">

  </div>
</div>
  